{
    "name": "flourishlabs/saloon-slack",
    "description": "Saloon connectors/requests for Slack",
    "keywords": [
        "flourishlabs",
        "saloon-slack"
    ],
    "homepage": "https://github.com/flourishlabs/saloon-slack",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashley Hindle",
            "email": "hello@flourishlabs.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "FlourishLabs\\SaloonSlack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlourishLabs\\SaloonSlack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "platform": {
            "php": "8.1.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
